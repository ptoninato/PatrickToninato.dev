{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": "off",
        "no-console": 0,
        "no-plusplus": 0,
        "max-len": "off",
        "import/extensions": "off",
        "no-continue": "off",
        "no-await-in-loop": "off"
    }
}
